#include <stdio.h>
#include <stdlib.h>

main()
{
	int i, num, lin;
	num = 0;
	lin = 80*23; //numero de linhas
	for (i=1;i<=80; i++)
	{
		printf("%c",178);
	}
	for (i=1;i<=lin; i++)
	{
		num++;
		if(num > 80)
		{
			num = num - 80;
		}
		if(num == 1 || num == 80 )
		{
			printf("%c",178);
		}
		else
		{
			printf(" ");
		}
	}
	for (i=1;i<=80; i++)
	{
		printf("%c",178);
	}
	system("pause > null");
}
