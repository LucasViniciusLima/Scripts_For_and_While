#include <math.h>
#include <stdio.h>
#include <stdlib.h>

main()
{
	int i, n;
	float form1, form2, form3, form4;
	for(i=1; i<=11; i++)
	{
		n = i;
		form1 = sqrt(5);
		form2 = pow(((1 + form1)/2),n);
		form3 = pow(((1 - form1)/2),n);
		form4 = ((form2 - form3)/form1);
		printf(" %.0f  ", form4);
	}
	system("pause > null");
}
